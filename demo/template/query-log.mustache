<div class="panel panel-default">
  <div class="panel-heading collapse">
    {{series_name}}
  </div>
  <div id="log-item-{{id}}" class="panel-collapse collapse">
    <div class="panel-body">
    <div><b>Total Time: </b> {{total_time}} ms </div>
    </div>
    <table class="table table-striped table-bordered">
      <thead>
        <th>Peer Name</th>
        <th>Count</th>
        <th>Scan Time</th>
        <th>Query Time</th>
      </thead>

      <tbody>
      {{#peers}}
      <tr>
        <td>{{name}}</td>
        <td>{{count}}</td>
        <td>{{duration}}ms</td>
        <td>{{full_duration}}ms</td>
      </tr>
      {{/peers}}
      </tbody> 
    </table>
  </div>
</div>
